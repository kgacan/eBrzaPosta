
@using BrzaPosta.ViewModels
@model DrzavaPrikaziVM.DrzavaVM

<br />

<div class="sadrzajDiv">

    <form>
        <div class="form-group">
            <fieldset>
                <label class="control-label" for="readOnlyInput">Region</label>
                <input asp-for="Region_Naziv" class="form-control" id="readOnlyInput" type="text" readonly="">
            </fieldset>
        </div>

        <div class="form-group">
            <fieldset>
                <label class="control-label" for="readOnlyInput2">Drzava</label>
                <input asp-for="Naziv" class="form-control" id="readOnlyInput2" type="text" readonly="">
            </fieldset>
        </div>

    </form>

    <div id="content"></div>

    <br />
    <a class="btn btn-outline-info" href='javascript:history.go(-1)'><i class="fas fa-backward"></i> Nazad</a>

</div>



<script>
    $(document).ready(function () {
        $.get("/Grad/Index?DrzavaId=@Model.DrzavaId",
            function (rezultat) {
                $("#content").html(rezultat);
            }
        )
    });
</script>
